<%= form_with(model: [@course, @coach], local: true) do |f| %>
 
   <div class="field">
    <%= f.label :image, "上半身写真" %><br />
    <%= f.file_field :image, id:"coach_image" %>
  </div>
  <div class="field">
    <%= f.label :coach_name, "コーチ氏名" %><br />
    <%= f.text_field :coach_name, id:"coach_name" %>
  </div>

  <div class="field">
    <%= f.label :coach_name_kana, "氏名（カナ）" %><br />
    <%= f.text_field :coach_name_kana, id:"coach_name_kana" %>
  </div>

  <div class="field">
    <%= f.label :homepoint_id, "ホームポイント" %><br />
    <%= f.text_field :homepoint_id, id:"homepoint_id" %>
  </div>

  <div class="field">
    <%= f.label :good_point, "実績等サブタイトル部分" %><br />
    <%= f.text_field :good_point, id:"good_point" %>
  </div>

  <div class="field">
    <%= f.label :profile, "プロフィール" %><br />
    <%= f.text_area :profile, id:"profile" %>
  </div>

  <div class="field">
    <%= f.label :course1, "実施コース１" %><br />
    <%= f.text_field :course1, id:"course1" %>
  </div>

  <div class="field">
    <%= f.label :course2, "実施コース2" %><br />
    <%= f.text_field :course2, id:"course2" %>
  </div>

  <div class="field">
    <%= f.label :course3, "実施コース3" %><br />
    <%= f.text_field :course3, id:"course3" %>
  </div>

  <div class="field">
    <%= f.label :course4, "実施コース4" %><br />
    <%= f.text_field :course4, id:"course4" %>
  </div>

  <div class="field">
    <%= f.label :course5, "実施コース5" %><br />
    <%= f.text_field :course5, id:"course5" %>
  </div>

  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>